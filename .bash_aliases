# Utility with github
alias daily_report='cd ~/Projects/github_daily && bundle exec ruby github_daily.rb grooves/crowd_agent $(date "+%Y/%m/%d")'

# Running local crowd_agent app accessible from Windows VM
alias rails-vm='cd ~/Projects/crowd_agent && bin/rails s -b 192.168.0.12 -p 3001'

# Many a little makes a mickle
alias yandle='yarn && bundle'
